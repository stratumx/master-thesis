TARGET ?= thesis
BIBTEX ?= bibtex8
BIBTEXFLAGS ?= -H -c cp1251

include ../../include/latex.mk

all: pdf

allpdf: pdf

alldvi: dvi

help: .help
	@echo "  all          alias for pdf target" ;\
	 echo "  allpdf       alias for pdf target" ;\
	 echo "  alldvi       alias for dvi target" ;\
